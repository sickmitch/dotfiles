{
	"pyprland": {
		"plugins": [
			"scratchpads",
			"expose"
		]
	},
	"scratchpads": {
		"term": {
			"command": "kitty --class kitty-dropterm",
			"unfocus": "hide",
			"animation": "fromTop",
      "margin": 47
		},
		"volume": {
			"command": "pavucontrol",
			"animation": "fromRight",
      "margin": 10,
			"unfocus": "hide"
		}
	}
}
