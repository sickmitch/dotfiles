{
	"pyprland": {
		"plugins": [
			"scratchpads",
			"expose"
		]
	},
	"scratchpads": {
		"term": {
			"command": "kitty --class kitty-dropterm",
      "position": "0% 50%",
			"unfocus": "hide",
			"animation": "fromTop"
		},
		"volume": {
			"command": "pavucontrol",
			"animation": "fromRight",
			"unfocus": "hide"
		}
	}
}
